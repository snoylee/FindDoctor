package com.guangyi.finddoctor.model;

public class Department {

	private String hdeptId;
	private String depaName;
	private String introduction;
	private String deptHospName;
	private int doctNum;
	private int guanghaoStatus;
	
	
	public static String HDEPTID = "hdeptId";
	public static String DEPANAME = "depaName";
	public static String INTRODUCTION = "introduction";
	public static String DOCTNUM = "doctNum";
	public static String DEPTHOSPNAME="deptHospName";
	public static String GUANGHAOSTATUS="guanghaoStatus";

	
	
	
	public int getGuanghaoStatus() {
		return guanghaoStatus;
	}

	public void setGuanghaoStatus(int guanghaoStatus) {
		this.guanghaoStatus = guanghaoStatus;
	}

	public String getDeptHospName() {
		return deptHospName;
	}

	public void setDeptHospName(String deptHospName) {
		this.deptHospName = deptHospName;
	}

	public String getIntroduction() {
		return introduction;
	}

	public void setIntroduction(String introduction) {
		this.introduction = introduction;
	}

	public String getHdeptId() {
		return hdeptId;
	}

	public void setHdeptId(String hdepaId) {
		this.hdeptId = hdepaId;
	}

	public String getDepaName() {
		return depaName;
	}

	public void setDepaName(String depaName) {
		this.depaName = depaName;
	}

	public int getDoctNum() {
		return doctNum;
	}

	public void setDoctNum(int doctNum) {
		this.doctNum = doctNum;
	}

}
