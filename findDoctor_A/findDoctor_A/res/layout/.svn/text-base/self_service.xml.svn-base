<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/relative_top_bar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/top_bar_background"
        android:gravity="center_vertical" >

        <TextView
            android:id="@+id/tv_topbar_center_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="自助服务"
            android:textColor="@color/white"
            android:textSize="@dimen/top_bar_center_text_size" />

          <Button
            android:id="@+id/ib_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="8dp"
            android:text="  返回"
            android:textColor="@color/white"
            android:gravity="center"
            android:background="@drawable/back_selector" />
    </RelativeLayout>

<LinearLayout 
    
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent" >
    
        <RelativeLayout
            android:id="@+id/search_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:background="@drawable/search_bg_self"
            android:orientation="horizontal"
            >

            <EditText
                android:id="@+id/editText1"
                android:layout_centerVertical="true"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/iv_search_btn"
                android:hint="搜索疾病、症状、化验检查等..."
                android:singleLine="true"
                android:textColor="#000"
                android:textSize="15sp"
                android:background="@drawable/search_edit_bg" >               
            </EditText>

            <ImageView
                android:id="@+id/iv_search_btn"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/search_btn_bg" />
            
        </RelativeLayout>
        
     
      
   
      <LinearLayout
        android:layout_weight="480"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@color/background"
        android:padding="10dp"
        android:orientation="vertical" >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp" >
     <RelativeLayout
    android:layout_width="0dp"
    android:layout_weight="1"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal">
    <Button
        android:id="@+id/self_service_item01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:background="@drawable/self_service_item01_selector" />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@+id/self_service_item01"
          android:gravity="center"
          android:layout_centerInParent="true"
          android:textColor="@color/black"
          android:text="症状自查" />
     </RelativeLayout>
     
     <RelativeLayout
    android:layout_width="0dp"
    android:layout_weight="1"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal">
    <Button
        android:id="@+id/self_service_item02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:background="@drawable/self_service_item02_selector"  />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@+id/self_service_item02"
          android:gravity="center"
          android:layout_centerInParent="true"
          android:textColor="@color/black"
          android:text="疾病库" />
     </RelativeLayout>
        </LinearLayout>
       <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp" >
     <RelativeLayout
    android:layout_width="0dp"
    android:layout_weight="1"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal">
    <Button
        android:id="@+id/self_service_item03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:background="@drawable/self_service_item03_selector"  />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@+id/self_service_item03"
          android:gravity="center"
          android:layout_centerInParent="true"
          android:textColor="@color/black"
          android:text="附近医院" />
     </RelativeLayout>
     
     <RelativeLayout
    android:layout_width="0dp"
    android:layout_weight="1"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal">
    <Button
        android:id="@+id/self_service_item04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:background="@drawable/self_service_item04_selector" />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@+id/self_service_item04"
          android:gravity="center"
          android:layout_centerInParent="true"
          android:textColor="@color/black"
          android:text="附近药店" />
     </RelativeLayout>
        </LinearLayout>
        
       <LinearLayout
            android:orientation="horizontal"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="0dp" >
     <RelativeLayout
    android:layout_width="0dp"
    android:layout_weight="1"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal">
    <Button
        android:id="@+id/self_service_item05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:background="@drawable/self_service_item05_selector"  />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@+id/self_service_item05"
          android:gravity="center"
          android:layout_centerInParent="true"
          android:textColor="@color/black"
          android:text="化验检查" />
     </RelativeLayout>
     
     <RelativeLayout
    android:layout_width="0dp"
    android:layout_weight="1"
    android:layout_height="wrap_content"
    android:visibility="invisible"
    android:gravity="center_horizontal">
     </RelativeLayout>
        </LinearLayout>

    </LinearLayout>
     
        
    </LinearLayout>

</LinearLayout>





